// Using collections and generics to access the data

class Code{

    public static void main(String... args) {

        ArrayList<Integer> a = new ArrayList<>();

        a.add(23);

        a.add(2);

        for (int i = 0; i < a.size(); i++) {

            System.out.print(a.get(i));

            System.out.print(", ");

        }

        System.out.println(a.isEmpty());

    }

    }

    //23, 2, false
